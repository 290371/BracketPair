{
  "name": "bracket-pair",
  "displayName": "Bracket Pair",
  "description": "Colorize matching brackets for pair identification",
  "version": "0.0.1",
  "publisher": "CoenraadS",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "BracketPair",
      "properties": {
        "bracketPair.pairColors": {
          "type": [
            "array"
          ],
          "items": {
            "type": [
              "array"
            ]
          },
          "default": [
            [
              "(",
              ")",
              [
                "#CCC42C",
                "#99976E",
                "#FFD351",
                "#90C2FF",
                "#2C9ECC"
              ],
              "red"
            ],
            [
              "[",
              "]",
              [
                "#CCC42C",
                "#99976E",
                "#FFD351",
                "#90C2FF",
                "#2C9ECC"
              ],
              "red"
            ],
            [
              "{",
              "}",
              [
                "#CCC42C",
                "#99976E",
                "#FFD351",
                "#90C2FF",
                "#2C9ECC"
              ],
              "red"
            ]
          ],
          "description": "[Open Bracket, Close Bracket, [Hex Colours], Orphan Color]"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  }
}